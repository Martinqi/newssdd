html,body{
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.top{
    width: 100%;
    height: 45px;
    line-height: 45px;
    text-align: center;
    background: linear-gradient(to left,#31c7ff,#32abff);
}
.content{
    width: 100%;
    height: 100%;
}
.content .left{
    width: 15%;
    height: 100%;
    background: #afd9ee;
    padding-left: 30px;
    padding-top: 30px;
    box-sizing: border-box;
    float: left;
}

ul ul {
    display: none;
}
ul ul li{
    padding:5px 10px;
    box-sizing: border-box;
}
ul ul li a{
    color: #999999;
}
iframe{
    width: 85%;
    height: 100%;
    padding-left: 50px;
    padding-top: 30px;
    box-sizing: border-box;
}
ul ul li:hover{
    background:#CBB9D3;

}
ul ul li a:hover{
    color: #fff;
}
.active{
    background:#CBB9D3;
    color: #fff;
}
.activeone{
    color: #fff;
}